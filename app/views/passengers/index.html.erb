<table>
<h2>Passenger List</h2>

  <tr>
    <th>ID</th>
    <th>Name</th>
    <th>Phone Number</th>
    <% @passengers.each do | passenger | %>
  </tr>
    <th><%= link_to passenger.id, passenger_path(passenger) %></th> 
    <th><%= link_to passenger.name, passenger_path(passenger.id) %></th> 
    <th><%= link_to passenger.phone_num, passenger_path(passenger) %></th> 
  <% end %>
  </tr>
</table>

